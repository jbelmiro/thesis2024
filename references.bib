  author = {Le Bourdonnec, Fran{\c c}ois-Xavier and Bontempi, Jean-Michel and Marini, Nathalie and Mazet, Sylvain and Neuville, Pierre Fran{\c c}ois and Poupeau, G{\'e}rard and Sicurani, Jean},
